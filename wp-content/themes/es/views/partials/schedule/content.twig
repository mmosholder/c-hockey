<div class="ch-tabbed-content">
  {% include 'partials/schedule/tabs.twig' %}
  {% for event in item.event_content %}
  <div id="tab{{ event.content_id }}" class="ch-tabbed-content-card {% if loop.first %}-active{% endif %}">
    <div class="ch-tabbed-content-container">
      <div class="ch-row">
        <div class="ch-heading-col">
          <h3>{{ event.event_day }}</h3>
        </div>
      </div>
      {% include 'partials/schedule/time-slot.twig' %}
    </div>
  </div>
  {% endfor %}
</div>
